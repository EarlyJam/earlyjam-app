{
  "plugins": [
    "@ianvs/prettier-plugin-sort-imports",
    "prettier-plugin-tailwindcss"
  ],
  "tailwindFunctions": ["cn", "clsx"],
  "tailwindPreserveDuplicates": true,
  "importOrder": [
    ".css$",
    "",
    "^react$",
    "",
    "<BUILTIN_MODULES>",
    "",
    "<THIRD_PARTY_MODULES>",
    "",
    "^@/",
    "",
    "^[../]",
    "^[./]",
    "",
    "<TYPES>^(node:)",
    "<TYPES>",
    "<TYPES>^[.]"
  ],
  "importOrderTypeScriptVersion": "5.2.2",
  "trailingComma": "none",
  "tabWidth": 2,
  "semi": true,
  "singleQuote": false
}
